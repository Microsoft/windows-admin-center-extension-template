{
    "$schema": "../node_modules/@microsoft/windows-admin-center-sdk/core/manifest/module-schema.json#",
    "name": "{!company-name}.{!product-name}",
    "displayName": "resources:strings:displayName",
    "description": "resources:strings:description",
    "target": "/modules/{!company-name}.{!module-name}",
    "icon": "sme-icon:icon-win-powerShell",
    "keywords": [
        "resources:strings:keywords"
    ],
    "entryPoints": [
        {

            "entryPointType": "solution",
            "name": "solution",
            "urlName": "{!primary-url-name}",
            "displayName": "resources:strings:solutionName",
            "description": "resources:strings:solutionDescription",
            "icon": "sme-icon:icon-win-powerShell",
            "rootNavigationBehavior": "connections",
            "connections": {
              "header": "resources:strings:connectionsListHeader",
              "connectionTypes": [
                "msft.sme.connection-type.server",
                "msft.sme.connection-type.windows-client"
              ]
            },
            "tools": {
              "enabled": true,
              "defaultTool": "home"
            }
          },
          {
          "entryPointType": "tool",
          "name": "home",
          "urlName": "{!secondary-url-name}",
          "displayName": "resources:strings:displayName",
          "description": "resources:strings:description",
          "icon": "sme-icon:icon-win-powerShell",
          "path": "",
          "requirements": [
                {
                  "solutionIds": [
                    "msft.sme.server-manager!servers",
                    "msft.sme.server-manager!windowsClients",
                    "{!company-name}.{!product-name}!solution"
                  ],
                  "connectionTypes": [
                    "msft.sme.connection-type.server",
                    "msft.sme.connection-type.windows-client"
                  ]
                }
              ]
        }
    ],
    "resources": [
